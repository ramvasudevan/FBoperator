	Doi = {10.1016/j.geomphys.2006.02.012},
	Issn = {0393-0440},
	Journal = {Journal of Geometry and Physics},
	Keywords = {Circuits},
	Number = {1},
	Pages = {209 - 250},
	Title = {{Dirac structures in Lagrangian mechanics part II: Variational structures}},
	Url = {http://www.sciencedirect.com/science/article/pii/S0393044006000246},
	Volume = {57},
	Year = {2006},
	Bdsk-Url-1 = {http://www.sciencedirect.com/science/article/pii/S0393044006000246},
	Bdsk-Url-2 = {http://dx.doi.org/10.1016/j.geomphys.2006.02.012}}

