	Fjournal = {Proceedings of The Royal Society of London. Series A. Mathematical, Physical and Engineering Sciences},
	Issn = {1364-5021},
	Journal = {Proc. R. Soc. Lond. Ser. A Math. Phys. Eng. Sci.},
	Mrclass = {37N20},
