	Journal = {Proceedings of the National Academy of Sciences},
	Number = {13},
	Pages = {4738-4743},
	Title = {Forecasting sudden changes in environmental pollution patterns},
	Url = {http://www.pnas.org/content/109/13/4738.abstract},
	Volume = {109},
	Year = {2012},
	Bdsk-Url-1 = {http://www.pnas.org/content/109/13/4738.abstract},
